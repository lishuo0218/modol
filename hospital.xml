    <geom type="box" size="5 0.02 0.1" pos="0 4.9 1" material="wood_mat"/>
    <geom type="box" size="0.02 5 0.1" pos="-4.9 0 1" material="wood_mat"/>
    <geom type="box" size="0.02 5 0.1" pos="4.9 0 1" material="wood_mat"/>

    <!-- 挂钟 -->
    <body pos="0 4.9 3.5">
       <geom type="cylinder" size="0.2 0.02" axis="0 1 0" rgba="1 1 1 1"/>
       <geom type="cylinder" size="0.22 0.02" axis="0 1 0" pos="0 0.01 0" rgba=".2 .2 .2 1"/>
