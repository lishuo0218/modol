    <!-- 检查灯（聚光灯效果，照向病床） -->
    <light pos="-2 0 4" dir="0 0 -1" diffuse="0.8 0.8 0.7" castshadow="true" cutoff="45"/>

    <!-- 地面 -->
    <geom type="plane" size="10 10 0.1" material="floor_mat"/>
    
    <!-- 墙壁 -->
    <geom type="box" size="5 0.1 3" pos="0 -5 1.5" material="wall_mat"/>
    <geom type="box" size="5 0.1 3" pos="0 5 1.5" material="wall_mat"/>
    <geom type="box" size="0.1 5 3" pos="-5 0 1.5" material="wall_mat"/>
    <geom type="box" size="0.1 5 3" pos="5 0 1.5" material="wall_mat"/>
    
    <!-- 墙面防撞条 -->
