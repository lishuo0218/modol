<mujoco model="hospital">
  <option timestep="0.005" integrator="RK4"/>

  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0"/>
    <rgba haze="0.15 0.25 0.35 1"/>
    <global azimuth="120" elevation="-20"/>
  </visual>

  <default>
    <geom rgba=".8 .9 .9 1"/>
    <default class="wall">
      <geom rgba=".7 .7 .7 1"/>
    </default>
  </default>

  <asset>
    <texture type="skybox" builtin="gradient" rgb1="0.9 0.95 1" rgb2="0.6 0.8 1" width="256" height="256"/>
    <texture name="floor_tex" type="2d" builtin="checker" rgb1=".9 .9 .9" rgb2=".8 .8 .8" width="512" height="512" mark="edge" markrgb=".5 .5 .5"/>
    <texture name="wood_tex" type="cube" builtin="flat" rgb1=".5 .4 .3" width="100" height="100"/>
    <texture name="metal_tex" type="cube" builtin="flat" rgb1=".8 .8 .8" width="100" height="100"/>
    
    <material name="floor_mat" texture="floor_tex" texrepeat="5 5" specular="0.1" shininess="0.2"/>
    <material name="wall_mat" rgba=".92 .96 .94 1" specular="0.1" shininess="0.1"/>
    <material name="wood_mat" texture="wood_tex" specular="0.2" shininess="0.3"/>
    <material name="metal_mat" texture="metal_tex" specular="0.8" shininess="0.8"/>
  </asset>

  <worldbody>
    <!-- 光源 -->
    <!-- 主环境光（暖色） -->
    <light pos="0 5 5" dir="0 -1 -1" diffuse="1 0.98 0.95" castshadow="false"/>
    <!-- 补光（冷色） -->
    <light pos="5 0 5" dir="-1 0 -1" diffuse="0.8 0.85 0.9" castshadow="false"/>
    <light pos="-5 0 5" dir="1 0 -1" diffuse="0.8 0.85 0.9" castshadow="false"/>
    <!-- 顶光 -->
