<mujoco model="nio_power_station">
  <statistic center="0 0 1.5" extent="10.0"/>

  <visual>
    <!-- 3. 光照响应优化: Improved Lighting Quality -->
    <!-- Increased shadow quality and resolution settings if supported by viewer -->
    <headlight diffuse="0.4 0.4 0.4" ambient="0.2 0.2 0.2" specular="0.1 0.1 0.1"/>
    <rgba haze="0.15 0.25 0.35 1"/>
    <global azimuth="-120" elevation="-20" offwidth="1920" offheight="1080"/>
    <quality shadowsize="4096" offsamples="8"/> <!-- Enable high-res shadows and MSAA -->
    <map shadowclip="0.5" shadowscale="0.5"/>
  </visual>

  <asset>
    <!-- Environment Assets -->
    <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/>
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3"
      markrgb="0.8 0.8 0.8" width="300" height="300"/>
    
    <!-- 1. 材质系统升级: PBR-inspired Material Calibration -->
    <!-- MuJoCo Material Model: 
         reflectance ~= Metallic/Specular intensity
         shininess ~= 1.0 / Roughness (High shininess = Low roughness)
         rgba ~= Albedo
    -->

    <material name="groundplane" texture="groundplane" texuniform="true" texrepeat="5 5" reflectance="0.1" shininess="0.1" specular="0.1"/>

    <!-- Metals (Silver/Chrome) -->
    <!-- High reflectance (0.8+), High specular (1.0), High shininess (0.8+) -->
    <material name="mat_wall_silver" rgba="0.9 0.9 0.92 1" reflectance="0.7" shininess="0.8" specular="1.0"/>
    <material name="mat_metal_grey" rgba="0.5 0.5 0.55 1" reflectance="0.6" shininess="0.6" specular="0.8"/>
    <material name="mat_metal_plate" rgba="0.6 0.6 0.65 1" reflectance="0.5" shininess="0.7" specular="0.6"/> <!-- Checker plate simulation -->
    
    <!-- Dielectrics (Plastics/Paints/Matte) -->
    <!-- Low reflectance (0.1-0.3), Moderate specular, Variable shininess -->
    <material name="mat_frame_black" rgba="0.1 0.1 0.12 1" reflectance="0.1" shininess="0.3" specular="0.3"/> <!-- Matte black metal -->
    <material name="mat_interior_dark" rgba="0.15 0.15 0.15 1" reflectance="0.05" shininess="0.1" specular="0.1"/> <!-- Dark matte plastic -->
    <material name="mat_wall_dark_grey" rgba="0.2 0.2 0.22 1" reflectance="0.1" shininess="0.2" specular="0.2"/>
    
    <!-- Glass (Transparent) -->
    <material name="mat_glass" rgba="0.6 0.7 0.8 0.3" reflectance="0.5" shininess="0.95" specular="1.0"/>
    
    <!-- Emissive Materials (Lighting) -->
    <material name="mat_light_strip" rgba="0.9 0.9 1.0 1" emission="1.0"/>
    <material name="mat_nio_blue_glow" rgba="0.0 0.8 1.0 1" emission="0.8"/>
    <material name="mat_screen_glow" rgba="0.2 0.6 1.0 1" emission="0.6"/>
    <material name="mat_light_green" rgba="0 1 0 1" emission="1.0"/>
    <material name="mat_light_red" rgba="1 0 0 1" emission="1.0"/>
    <material name="mat_lcd_display" rgba="0.1 0.1 0.2 1" emission="0.4"/>
    <material name="mat_text_white" rgba="1 1 1 1" emission="0.5"/>

    <!-- Functional Colors -->
    <material name="mat_warning_yellow" rgba="0.95 0.7 0.05 1" reflectance="0.2" shininess="0.4" specular="0.2"/>
    <material name="mat_battery_pack" rgba="0.15 0.15 0.15 1" reflectance="0.2" shininess="0.5" specular="0.4"/>
    <material name="mat_floor_concrete" rgba="0.4 0.4 0.42 1" reflectance="0.1" shininess="0.1" specular="0.1"/> <!-- Rough concrete -->
    <material name="mat_door_seam" rgba="0.02 0.02 0.02 1" reflectance="0.0"/>
    
    <!-- Mechanical Parts -->
    <material name="mat_mechanical_blue" rgba="0.05 0.2 0.5 1" reflectance="0.4" shininess="0.6" specular="0.6"/> <!-- Painted metal -->
    <material name="mat_roller_yellow" rgba="0.9 0.7 0.1 1" reflectance="0.3" shininess="0.5" specular="0.4"/> <!-- Hard plastic/metal -->
    <material name="mat_lidar_window" rgba="0.05 0.05 0.05 1" reflectance="0.2" shininess="0.9" specular="0.8"/> <!-- Glossy black plastic -->

    <!-- Sinopec Materials - Calibrated -->
    <material name="mat_sinopec_red" rgba="0.85 0.1 0.1 1" reflectance="0.3" shininess="0.7" specular="0.5"/> <!-- Glossy plastic/metal -->
    <material name="mat_sinopec_white" rgba="0.92 0.92 0.92 1" reflectance="0.4" shininess="0.6" specular="0.5"/>
    <material name="mat_pump_island" rgba="0.2 0.2 0.2 1" reflectance="0.1" shininess="0.2" specular="0.2"/> <!-- Matte -->
    <material name="mat_pump_screen" rgba="0.05 0.05 0.15 1" emission="0.3"/>
    <material name="mat_hose_black" rgba="0.1 0.1 0.1 1" reflectance="0.05" shininess="0.2" specular="0.1"/> <!-- Rubber -->
    <material name="mat_hose_nozzle_green" rgba="0.0 0.5 0.0 1" reflectance="0.2" shininess="0.4" specular="0.3"/>
    <material name="mat_hose_nozzle_red" rgba="0.7 0.0 0.0 1" reflectance="0.2" shininess="0.4" specular="0.3"/>
    <material name="mat_hose_nozzle_yellow" rgba="0.9 0.8 0.0 1" reflectance="0.2" shininess="0.4" specular="0.3"/>
    <material name="mat_pump_panel_grey" rgba="0.6 0.6 0.65 1" reflectance="0.3" shininess="0.4" specular="0.3"/>
    
    <!-- Classroom Materials -->
    <material name="mat_class_wall_white" rgba="0.95 0.95 0.95 1" reflectance="0.1" shininess="0.1" specular="0.1"/>
    <material name="mat_class_floor_wood" rgba="0.6 0.4 0.25 1" reflectance="0.1" shininess="0.2" specular="0.1"/>
    <material name="mat_blackboard" rgba="0.1 0.25 0.15 1" reflectance="0.05" shininess="0.1" specular="0.1"/>
    <material name="mat_desk_top" rgba="0.8 0.7 0.5 1" reflectance="0.2" shininess="0.3" specular="0.2"/>
    <material name="mat_chair_blue" rgba="0.2 0.3 0.5 1" reflectance="0.1" shininess="0.3" specular="0.2"/>
    <material name="mat_podium" rgba="0.4 0.25 0.15 1" reflectance="0.2" shininess="0.3" specular="0.2"/>
    <material name="mat_window_glass" rgba="0.7 0.8 0.9 0.2" reflectance="0.3" shininess="0.9" specular="0.8"/>
  </asset>

